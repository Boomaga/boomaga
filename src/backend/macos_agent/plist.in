<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

  <key>Label</key>
  <string>io.github.Boomaga.agent</string>

  <key>LimitLoadToSessionType</key>
  <array>
    <string>LoginWindow</string>
    <string>Aqua</string>
  </array>

  <key>RunAtLoad</key>
  <false/>

  <key>ProgramArguments</key>
  <array>
    <string>/Library/Printers/@MAC_APP_ID@/agent</string>
    <string>@MAC_SPOOL_DIR@</string>
    <string>~/Library/Application Support/@MAC_APP_ID@</string>
    <string>@MAC_APP_ID@</string>
  </array>

  <key>KeepAlive</key>
  <false/>

  <key>EnableGlobbing</key>
  <true/>

  <key>WatchPaths</key>
  <array>
    <string>@MAC_SPOOL_DIR@/.start</string>
  </array>

<!--
  <key>EnvironmentVariables</key>
  <dict>
    <key>DEBUG</key><string>1</string>
  </dict>

  <key>StandardOutPath</key>
  <string>/tmp/@MAC_APP_ID@.agent.log</string>

  <key>StandardErrorPath</key>
  <string>/tmp/@MAC_APP_ID@.agent.log</string>
-->

</dict>
</plist>

